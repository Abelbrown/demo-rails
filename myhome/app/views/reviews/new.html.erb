<h1 class="text-center">Ajouter une review pour <%= @restaurant.name %></h1>



<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-6">

      <%= form_for [@restaurant, @review] do |f| %>
        <%= f.text_field :content %>
        <%= f.submit%>
      <% end %>

    </div>
  </div>
</div>





